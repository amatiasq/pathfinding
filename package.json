  "name": "amq-pathfinding",
  "version": "0.1.0",
  "description": "",
  "jest": {
    "mapCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "\\.spec\\.ts$",
    "testPathIgnorePatterns": [ ".bakup" ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "main": "src/main.ts",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^19.2.3",
    "jest": "^20.0.4",
    "ts-jest": "^20.0.4",
    "ts-loader": "^2.0.3",
    "tslint": "^5.3.2",
    "tslint-language-service": "^0.9.6",
    "typescript": "^2.3.3",
    "webpack": "^2.4.1"
  },
  "scripts": {
    "test": "npm run lint && jest",
    "test:ci": "jest",
    "test:watch": "jest",
    "build": "cd config; webpack --config ./webpack.config.js",
    "lint": "tslint -c tslint.json --type-check --project tsconfig.json 'src/**/*.ts'"
  },
